import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int base = sc.nextInt();
        int exponent = sc.nextInt();
        int ans = 1;
        
        if (exponent == 0) {
            System.out.println(1);
        }
        else {
            for (int i = 1; i <= exponent; i++) {
                ans *= base;
            }
            System.out.println(ans);
        }
    }
}
